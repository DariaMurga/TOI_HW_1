using System;
using System.IO;

namespace TOI_HW1
{
    class MainClass
    {
        public static void Main(string[] args)
        {
            byte[] p = File.ReadAllBytes(@"");

            int[] t = new int[256];
            for (int i = 0; i < p.Length; i++)
            {
                int count = p[i];
                t[count] += 1;
            }

            FileStream f = new FileStream("dx.txt", FileMode.Create);
            foreach (var a in t)
            {
                Console.WriteLine(a);
            }
            Console.ReadKey();
        }
    }
}
